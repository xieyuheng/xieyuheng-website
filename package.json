{
  "name": "xieyuheng-website",
  "version": "0.0.1",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite",
    "build:app": "vite build",
    "build": "npm run build:app",
    "check": "vue-tsc --noEmit",
    "check:watch": "while true; do vue-tsc --noEmit --watch && break; done",
    "check:ts": "tsc --noEmit",
    "check:ts:watch": "tsc --noEmit --watch",
    "test": "vitest --run",
    "format": "prettier src --write"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "@vueuse/core": "^10.4.1",
    "@vueuse/head": "^2.0.0",
    "tailwindcss": "^3.3.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "qs": "^6.11.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.3.4",
    "autoprefixer": "^10.4.15",
    "@types/qs": "^6.9.8",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "pretty-quick": "^3.1.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-rewrite-all": "^1.0.1",
    "vitest": "^0.34.4",
    "vue-tsc": "^1.8.11"
