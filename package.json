{
  "name": "xieyuheng-website",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite",
    "build:app": "vite build",
    "build": "npm run build:app",
    "check": "vue-tsc --noEmit",
    "check:watch": "while true; do vue-tsc --noEmit --watch && break; done",
    "check:ts": "tsc --noEmit",
    "check:ts:watch": "tsc --noEmit --watch",
    "test": "vitest --run",
    "format": "prettier src --write"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.1.5",
    "@vueuse/core": "^11.2.0",
    "@vueuse/head": "^2.0.0",
    "tailwindcss": "^3.4.14",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "qs": "^6.13.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "autoprefixer": "^10.4.20",
    "@types/qs": "^6.9.16",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "pretty-quick": "^4.0.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.4",
    "vue-tsc": "^2.1.10"
  }
}
