{
  "name": "xieyuheng-website",
  "version": "0.0.1",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite",
    "build:app": "vite build",
    "build": "npm run build:app",
    "check": "vue-tsc --noEmit",
    "check:watch": "while true; do vue-tsc --noEmit --watch && break; done",
    "check:ts": "tsc --noEmit",
    "check:ts:watch": "tsc --noEmit --watch",
    "test": "vitest --run",
    "format": "prettier src --write"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.22",
    "@heroicons/vue": "^2.1.3",
    "@vueuse/core": "^10.10.0",
    "@vueuse/head": "^2.0.0",
    "tailwindcss": "^3.4.4",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2",
    "qs": "^6.12.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.5",
    "autoprefixer": "^10.4.19",
    "@types/qs": "^6.9.15",
    "postcss": "^8.4.38",
    "prettier": "^3.3.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.6.1",
    "pretty-quick": "^4.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.19"
  }
}
